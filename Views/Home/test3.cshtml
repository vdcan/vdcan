@{
    //------------------------------------------------------------------------------
    //       时间： 2017-09-25
    //       作者： 蔡捷
    //
    //       文件： breed_application.cshtml 页面文件
    //------------------------------------------------------------------------------
    ViewBag.Title = "注册";
    Layout = "~/Views/Shared/_Layout_noui.cshtml";
}
@section head{
    <style type="text/css">
        #toolbar1, #toolbar2 {
            border-bottom: none;
        }

        .table_content {
            width: 100%;
            background: #fff;
            border: none;
        }

            .table_content td {
                border-bottom: 1px solid #ccc;
            }

                .table_content td.td_title {
                    width: 90px;
                    text-align: center;
                    font-size: 12px;
                    font-weight: bold;
                    background: #fafafa;
                    border-right: 1px solid #ccc;
                }

        .textbox, .textbox-text {
            border: none;
            border-radius: 0px;
            box-shadow: none;
            margin: 0px;
            padding: 0px;
        }

        .textbox-fucused {
            box-shadow: none;
        }

        #breed_application_content {
            display: none;
        }

        .img_g {
            float: left;
        }

        .delete {
            color: red;
        }

        .delete_gene {
            color: red;
        }

        #login {
            width: 300px;
            height: auto;
            margin: 0 auto;
            margin-top: 100px;
        }

        #login_panel {
            margin-top: 20px;
            background: #FFF;
            width: 400px;
            border: 1px solid #CCC;
            overflow: hidden;
            -moz-border-radius: 8px;
            -webkit-border-radius: 8px;
            border-radius: 8px;
            -moz-box-shadow: 1px 1px 1px rgba(0,0,0,.2);
            -webkit-box-shadow: 1px 1px 1px rgba(0,0,0,.2);
            box-shadow: 1px 1px 1px rgba(0,0,0,.2);
        }

            #login_panel .inputbox {
                width: 320px;
                height: 40px;
                margin: 10px auto;
            }

            #login_panel input {
                width: 275px;
                height: 22px;
                line-height: 22px;
                border: 1px solid #ccc;
                padding: 5px 2px;
            }

            #login_panel .login_actions {
                background: #E8E8E8;
                width: auto;
                padding: 15px;
                border-top: 1px dotted #CCC;
                text-align: center;
            }

        .login_msg {
            width: 300px;
            height: auto;
            margin: 0 auto;
            margin-top: 20px;
            text-align: center;
        }
    </style>
}

@section scripts{

    <script src="~/Scripts/uploadifive.min.js"></script>
    <script src="~/Scripts/jquery.viewbox.min.js"></script>
    <link href="~/Scripts/viewbox.css" rel="stylesheet" />
    <script src="~/Areas/Dev/ViewJS/breed_application.js?v=@ViewBag.ts"></script>

    <script type="text/javascript">
        var model_string = '@Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model))';
        km.model = JSON.parse(model_string);

        km.pageSize = @ViewBag.Settings["gridrows"];
    </script>
}




<div id="login">
    <img src="/Content/images/km_logo_easyui.png" />
    <div id="login_panel">


        <form id="breed_application_content" class="form_content" method="post">
            <table class="table_content" cellpadding="3" cellspacing="0" border="0">
                <input id='TPL_id' name='id' type='hidden' value='0' />
                <tr><td class="td_title">用户名</td> <td>  <div class="inputbox"><input type="text" data-options="required:true" id="UserCode" name="UserCode" value="" tabindex="1" placeholder="请输入用户名" /></div></td></tr>
                <tr><td class="td_title">姓名</td> <td>  <div class="inputbox"><input type="text" id="RealName" name="RealName" value="" tabindex="2" placeholder="请输入姓名" /></div></td></tr>
                <tr><td class="td_title">单位名称</td> <td>  <div class="inputbox"><input type="text" data-options="required:true" id="DepartmentName" name="DepartmentName" value="" tabindex="3" placeholder="请输入单位名称" /></div></td></tr>
                <tr><td class="td_title">电话</td> <td> <div class="inputbox"><input type="text" id="Phone" name="Phone" value="" tabindex="4" /></div></td></tr>
                <tr><td class="td_title">电子邮件</td> <td>  <div class="inputbox"><input type="text" id="Email" name="Email" data-options="validType:['email'] ,multiline:true" value="" tabindex="5" /></div></td></tr>
                <tr><td class="td_title">地址</td> <td> <div class="inputbox"><input type="text" id="Address" name="Address" value="" tabindex="6" /></div></td></tr>
                <tr><td class="td_title">传真</td> <td> <div class="inputbox"><input type="text" id="Fax" name="Fax" value="" tabindex="7" /></div></td></tr>


            </table>

        </form>
        <div class="login_actions">

            <a href="javascript:void(0)" class="cssbtn cssbtn_size12 cssbtn_green" title="注册" style=" padding:8px 50px" onclick="km.toolbar.do_login();">注册</a>
            <a href="/Login" class="cssbtn cssbtn_size12 cssbtn_green" title="取消" style=" padding:8px 50px">取消</a>
        </div>
    </div>
</div>
<div class="login_msg"></div>
<div style=" position:absolute; z-index:10; bottom:0px; width:100%; height:32px; line-height:32px; text-align:center; color:#808080">
    <span id="span_ip"></span>
</div>


@*</div>*@

