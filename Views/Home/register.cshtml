@{

    // Layout = "~/Views/Shared/_Layout_static.cshtml";
    Layout = "~/Views/Home/_main_layout.cshtml";
}
@section head{
    <style type="text/css">
        #toolbar1, #toolbar2 {
            border-bottom: none;
        }

        .table_content {
            width: 100%;
            #background: #fff;
            border: none;
        }

            .table_content td {
                #border-bottom: 1px solid #ccc;
            }

                .table_content td.td_title {
                    width: 90px;
                    text-align: right;
                    font-size: 12px;
                    font-weight: bold;
                    #background: #fafafa;
                    #border-right: 1px solid #ccc;
                }

        .textbox, .textbox-text {
            border: none;
            border-radius: 0px;
            box-shadow: none;
            margin: 0px;
            padding: 0px;
        }

        .textbox-fucused {
            box-shadow: none;
        }

        #breed_application_content {
            display: none;
        }

        .img_g {
            float: left;
        }

        .delete {
            color: red;
        }

        .delete_gene {
            color: red;
        }

        #login {
            width: 300px;
            height: auto;
            margin: 0 auto;
            margin-top: 100px;
        }

        #login_panel {
            margin-top: 20px;
            background: #FFF;
            width: 400px;
            #border: 1px solid #CCC;
            overflow: hidden;
            #-moz-border-radius: 8px;
            #-webkit-border-radius: 8px;
            border-radius: 8px;
            #-moz-box-shadow: 1px 1px 1px rgba(0,0,0,.2);
            #-webkit-box-shadow: 1px 1px 1px rgba(0,0,0,.2);
            #box-shadow: 1px 1px 1px rgba(0,0,0,.2);
        }

            #login_panel .inputbox {
                width: 320px;
                #height: 40px;
                margin: 10px auto;
            }

            #login_panel input {
                width: 275px;
                height: 22px;
                line-height: 22px;
                border: 1px solid #ccc;
                padding: 5px 2px;
            }

            #login_panel .login_actions {
                #background: #E8E8E8;
                width: auto;
                padding: 15px;
                #border-top: 1px dotted #CCC;
                #text-align: right;
                text-align: center;
            }

        .login_msg {
            width: 300px;
            height: auto;
            margin: 0 auto;
            margin-top: 20px;
            text-align: center;
        }

        .language {
            float: right;
            position: absolute;
            top: 10px;
            right: 10px;
        }
        .td_title{
            float:left;
            margin:10px;
            text-align:right;
        }
    </style>
}

